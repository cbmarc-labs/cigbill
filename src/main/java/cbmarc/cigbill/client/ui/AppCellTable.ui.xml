<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">

	<ui:style>
		
	</ui:style>

	<g:HTMLPanel>
		
		<c:CellTable ui:field="cellTable" pageSize="10" />

		<g:HTMLPanel>
			<span class="pull-right">
				<b:Pagination>
					<b:NavLink ui:field="fastBackward" icon="FAST_BACKWARD"></b:NavLink>
					<b:NavLink ui:field="backward" icon="BACKWARD"></b:NavLink>
					<b:NavLink ui:field="count" disabled="true">0-100:250</b:NavLink>
					<b:NavLink ui:field="forward" icon="FORWARD"></b:NavLink>
					<b:NavLink ui:field="fastForward" icon="FAST_FORWARD"></b:NavLink>
				</b:Pagination>
			</span>
		</g:HTMLPanel>
		
	</g:HTMLPanel>
</ui:UiBinder> 