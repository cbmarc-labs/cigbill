<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client">

	<ui:style>
		
	</ui:style>

	<g:HTMLPanel>

		<c:CellTable ui:field="cellTable" pageSize="10" />

		<g:HTMLPanel ui:field="pagination">

			<span class="pull-right">

				<div class="pagination">
					<ul>
						<li>
							<g:Anchor href="javascript:;" ui:field="fastBackward">
								<i class="icon-fast-backward"></i>
							</g:Anchor>
						</li>

						<li>
							<g:Anchor href="javascript:;" ui:field="backward">
								<i class="icon-backward"></i>
							</g:Anchor>
						</li>

						<li class="disabled">
							<g:Anchor ui:field="count" />
						</li>

						<li>
							<g:Anchor href="javascript:;" ui:field="forward">
								<i class="icon-forward"></i>
							</g:Anchor>
						</li>

						<li>
							<g:Anchor href="javascript:;" ui:field="fastForward">
								<i class="icon-fast-forward"></i>
							</g:Anchor>
						</li>
					</ul>
				</div>
			</span>

		</g:HTMLPanel>

	</g:HTMLPanel>

</ui:UiBinder> 